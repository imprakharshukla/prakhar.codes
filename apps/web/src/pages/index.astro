---
import BaseHead from "../components/BaseHead.astro";
import { SITE_TITLE, SITE_DESCRIPTION } from "../consts";
import { getCollection } from "astro:content";
import PostFeed from "../components/PostFeed";
import ProjectFeed from "../components/ProjectFeed";
import Layout from "../layouts/layout.astro";
import GitHubStatsBentoGrid from "../components/GithubStatsBentoGrid";
import IconButton from "../components/IconButton.astro";
import ResumeButton from "../components/ResumeButton";
import LocalTimePill from "../components/LocalTimePill";
import LocationPill from "../components/LocationPill";
import ProfileAvatar from "../components/ProfileAvatar";
import HiringPopover from "../components/HiringPopover";
import CalEmbed from "../components/CalEmbed";
import FilePdfIcon from "../components/icons/FilePdfIcon";
import CalendarIcon from "../components/icons/CalendarIcon";
import GithubLogoIcon from "../components/icons/GithubLogoIcon";
import XLogoIcon from "../components/icons/XLogoIcon";
import LinkedinLogoIcon from "../components/icons/LinkedinLogoIcon";
import MoreTechChip from "../components/MoreTechChip";

const blogPosts = await getCollection("blog");
const projects = await getCollection("project");

const filteredBlogPosts = blogPosts
  .filter((post) => post.data.publish === true)
  .sort(
    (a, b) =>
      new Date(b.data.pubDate).getTime() - new Date(a.data.pubDate).getTime()
  );
---

<!doctype html>
<html lang="en">
  <head>
    <BaseHead
      title="Prakhar Shukla - Serial Founder & Software Engineer"
      description="Serial founder and Founding Engineer with 2.5M+ downloads and $140K+ revenue. Recently at Yobr (Norway). Building Andronix and Lumoflo. Full-stack, AI/ML, TypeScript, React, Node.js."
    />
  </head>
  <Layout>
    <div class="grid gap-2 md:max-w-2xl mt-6 animate-in fade-in duration-200">
      <!-- Hero Image -->
      <div class="w-full mb-8">
        <img
          width="1500"
          height="500"
          class="rounded-2xl shadow-lg w-full h-auto object-contain rotate-[1.5deg]"
          src="https://imagedelivery.net/nW2YfjVFqxC-ok-_zXWXQw/eed88e28-99e1-46ec-2bcc-d5b098a29900/hd"
          alt="Hero"
        />
      </div>

      <!-- Old layered images with hover effects (commented out) -->
